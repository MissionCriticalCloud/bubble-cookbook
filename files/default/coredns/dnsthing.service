[Unit]
Description=Manage dynamic dns for docker containers
After=docker.service

[Service]
PIDFile=/var/run/dnsthing.pid
User=coredns
WorkingDirectory=/home/coredns
ExecStart=/usr/bin/python /opt/coredns/dnsthing.py -H /home/coredns/docker.cloud.lan --debug
Restart=always

[Install]
WantedBy=multi-user.target
